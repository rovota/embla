{
   "name": "rovota/embla",
   "version": "1.0.0",
   "description": "A lightweight component library, helping you to build modern interfaces more efficiently.",
   "keywords": ["components", "interface", "php", "rovota", "modern", "web"],
   "homepage": "https://github.com/rovota/embla",
   "authors": [
      {
         "name": "LÃ©andro Tijink"
      }
   ],
   "type": "library",
   "license": "MIT",
   "require": {
      "php-64bit": "^8.4",
      "illuminate/support": "^12.0"
   },
   "autoload": {
      "psr-4": {
         "Rovota\\Embla\\": "src/"
      },
      "files": ["src/helpers.php"]
   },
   "extra": {
      "laravel": {
         "providers": [
            "Rovota\\Embla\\Providers\\EmblaServiceProvider"
         ]
      }
   },
   "config": {
      "sort-packages": true,
      "optimize-autoloader": true
   }
}