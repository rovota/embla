{
   "name": "rovota/embla",
   "version": "1.0.4",
   "description": "A lightweight component library, helping you to build modern interfaces more efficiently.",
   "keywords": ["components", "interface", "php", "laravel", "modern", "web"],
   "homepage": "https://github.com/rovota/embla",
   "authors": [
      {
         "name": "LÃ©andro Tijink"
      }
   ],
   "type": "library",
   "license": "MIT",
   "repositories": [
      {
         "type": "composer",
         "url": "https://rovota.repo.repman.io"
      }
   ],
   "require": {
      "php-64bit": "^8.4||^8.5",
      "ext-mbstring": "*",
      "laravel/framework": "^12.0"
   },
   "autoload": {
      "psr-4": {
         "Rovota\\Embla\\": "src/"
      },
      "files": ["src/helpers.php"]
   },
   "extra": {
      "laravel": {
         "providers": [
            "Rovota\\Embla\\EmblaServiceProvider"
         ],
         "aliases": {
            "Tabs": "Rovota\\Embla\\Facades\\Tabs"
         }
      }
   },
   "config": {
      "sort-packages": true,
      "optimize-autoloader": true
   }
}